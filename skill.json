{
  "manifest": {
    "publishingInformation": {
      "isAvailableWorldwide": false,
      "distributionCountries": [
        "US"
      ],
      "distributionMode": "PUBLIC",
      "locales": {
        "en-US": {
          "largeIconUri": "https://raw.githubusercontent.com/steveoh/alexa-how-high-am-i/master/assets/large.png",
          "smallIconUri": "https://raw.githubusercontent.com/steveoh/alexa-how-high-am-i/master/assets/small.png",
